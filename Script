--AUTO ACHV (WORK IN PROGRESS)
RebirthSection:NewToggle(BoLNLVkzNuOukmiWUnUqminOigxwmrAiavCmzKOlMmcv('NWHnvxkvEddIPPjWLXnqbrnkSIVqqQasaYFFShZEDEqfQclDqBTkcDUQXV0byBDbGFpbSBBbGwgVHJvcGh5cyBSZXdhcmRz'), BoLNLVkzNuOukmiWUnUqminOigxwmrAiavCmzKOlMmcv('beVUuzAvLDEeMsodXWhqtmOCAthWlydjGCwBvLNsLYvbXikZGKRsxlqVGhpcyBhdXRvbWF0aWNhbGx5IGNsYWltcyBldmVyeSBUcm9waHkgUmV3YXJk'), function(state)
    if state then
        for i = 1, 48 do -- This starts at 1 and fires the same arg all the way up to 48
            local args = {
                [1] = BoLNLVkzNuOukmiWUnUqminOigxwmrAiavCmzKOlMmcv('RzHGTAeGfdBJYoqJciJuxwrLCarAdZybgGiuIBUaRIDOtkpfsyDAgnxZ2V0X2FjaF9yZXdhcmQ='),
                [2] = {
                    [BoLNLVkzNuOukmiWUnUqminOigxwmrAiavCmzKOlMmcv('NOkkLLjgufJcvAvtNauubFUNgBleZFgSwAozcJpQHIEbRkDnsnKqbZkMQ==')] = i, -- Current iteration (you can change `i` to whatever you want)
                },
            }
        
            game:GetService(BoLNLVkzNuOukmiWUnUqminOigxwmrAiavCmzKOlMmcv('gBNGCwVXYZvnnyemyvOGwJQJasiPBkuaOphsOKXJSqQxymZnCsphpSZUmVwbGljYXRlZFN0b3JhZ2U=')).RemoteFunction:InvokeServer(unpack(args))
            task.wait() --How much time in between fires
        end
    else
        print(BoLNLVkzNuOukmiWUnUqminOigxwmrAiavCmzKOlMmcv('DJSvQfuQrZWicRDXxoxowqpjBVBdKmOnYtzySqpcBwcLMfoKaLrbIljVG9nZ2xlIE9mZg=='))
    end
end)

